<html>
<head>
  <title>M.F.C</title>
  <%= stylesheet_link_tag "application","jquery-ui-1.8.21.custom", :media => "all" %>
  <%= javascript_include_tag "application","customize_MFC","jquery-ui-1.8.21.custom.min"%>
  

  <%= csrf_meta_tags %>
</head>

<body>
 <p class="alert"><%= alert %></p>
<p class="notice"><%= notice %></p>
 <% if user_signed_in? %>
 	Hi, you are signed in with <%= current_user.email %> |
  	<%= link_to "Sign out", destroy_user_session_path, :method => :delete %>
 <% end %>	
 <br/> 
 <%= yield %>  
</body>

</html>
